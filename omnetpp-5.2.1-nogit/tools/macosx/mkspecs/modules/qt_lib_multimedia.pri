QT.multimedia.VERSION = 5.6.1
QT.multimedia.MAJOR_VERSION = 5
QT.multimedia.MINOR_VERSION = 6
QT.multimedia.PATCH_VERSION = 1
QT.multimedia.name = QtMultimedia
